{
  "name": "coffeestack",
  "preferGlobal": false,
  "private": true,
  "version": "0.0.1",
  "description": "just another CoffeeStack project",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fabriziomoscon/coffeestack"
  },
  "scripts": {
    "predeploy": "./node_modules/.bin/jake siteminify",
    "prestart": "./node_modules/.bin/jake siteminify",
    "start": "export NODE_PATH=$NODE_PATH:./ && coffee app.coffee",
    "supervisor": "TZ='UTC' NODE_PATH=$NODE_PATH:./ supervisor app.coffee",
    "testing": "TZ='UTC' NODE_ENV=testing NODE_PATH=$NODE_PATH:./ supervisor -i ciao app.coffee",
    "front-test": "TZ='UTC' NODE_ENV=testing ./node_modules/.bin/jake jstest",
    "back-test": "TZ='UTC' NODE_ENV=testing NODE_PATH=$NODE_PATH:./ mocha test/unit",
    "test": "npm run back-test && coffee test/lint.coffee && npm run front-test"
  },
  "dependencies": {
    "mongodb": "1.3.x",
    "underscore": "1.4.x",
    "coffee-script": "1.6.x",
    "express": "3.2.x",
    "socket.io": "0.9.x",
    "stylus": "0.32.x",
    "hogan.js": "2.0.0",
    "date-utils": "*",
    "connect-mongo": "*",
    "numeral": "1.4.x",
    "node-markdown": "~0.1.1",
    "browserify": "2.14.x",
    "check-types": "0.6.x",
    "request": "2.21.x",
    "q": "0.9.x",
    "mmm-vanillahogan": "0.2.x",
    "async": "0.2.x",
    "supervisor": "*",
    "validator": "*",
    "bunyan": "*"
  },
  "devDependencies": {
    "jake": "0.5.x",
    "coffeelint": "0.5.x",
    "mocha": "1.10.x",
    "assert": "0.4.x",
    "should": "1.2.x",
    "chai": "1.5.x",
    "one": "3.0.x",
    "uglify-js": "2.2.x",
    "watch": "0.7.x",
    "breakdown": "*",
    "walk": "~2.2.1",
    "soap": "*"
  },
  "engines": {
    "node": "0.8.x",
    "npm": "1.1.x"
  },
  "domains": [
    "yourproject.com"
  ],
  "subdomain": "subdomain.yourproject.com"
}